    "surface_rule": {
        
        "type": "minecraft:sequence",
        "sequence": [
            {
                "type": "minecraft:condition",
                "if_true": {
                    "type": "minecraft:vertical_gradient",
                    "false_at_and_above": {
                        "above_bottom": 5
                    },
                    "random_name": "minecraft:bedrock_floor",
                    "true_at_and_below": {
                        "above_bottom": 0
                    }
                }, 
                "then_run": {
                    "type": "minecraft:block",
                    "result_state": {
                        "Name": "minecraft:bedrock"
                    }
                }
            },
            {
                "type": "minecraft:condition",
                "if_true": {
                    "type": "biome",
                    "biome_is": ["jy_overworld:taiga"]
                }, 
                "then_run": {
                    "type": "minecraft:block",
                    "result_state": {
                        "Name": "minecraft:grass_block"
                    }
                }
            },
            {
                "type": "minecraft:condition",
                "if_true": {
                    "type": "biome",
                    "biome_is": ["jy_overworld:beach"]
                }, 
                "then_run": {
                    "type": "minecraft:block",
                    "result_state": {
                        "Name": "minecraft:sand"
                    }
                }
            },
            {
                "type": "minecraft:condition",
                "if_true": {
                    "type": "biome",
                    "biome_is": ["jy_overworld:ocean"]
                }, 
                "then_run": {
                    "type": "minecraft:block",
                    "result_state": {
                        "Name": "minecraft:gravel"
                    }
                }
            }
